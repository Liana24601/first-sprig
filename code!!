    const player = "p";
    const goal = "g";
    const wall = "w";

setLegend(
    [ player, bitmap``],
    [ wall, bitmap``],
    [ goal, bitmap``]
);


setSolids([])

let level = 0
const levels = [
	map``,
    map``,
]

setMap(levels[level])

onInput("s", () => {
	getFirst(player).y += 1
})

onInput("w", () => {
	getFirst(player).y -= 1
})

onInput("d", () => {
	getFirst(player).x += 1
})
